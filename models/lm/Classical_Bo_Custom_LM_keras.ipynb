    "from pathlib import Path\n",
    "\n",
    "def _mkdir(path: Path) -> Path:\n",
    "  path.mkdir(exist_ok=True, parents=True)\n",
    "  return path\n",
    "\n",
    "BASE_PATH = Path(\"/content/drive/MyDrive/OpenPecha/ML/LM\")\n",
