        "def generate_xy_pairs(seq, max_len):\n",
        "    x, y = [], []\n",
        "    for i, tok_id in enumerate(seq):\n",
        "      x_padded = pad_sequences([seq[:i]], maxlen=max_len)[0]\n",
        "      x.append(x_padded)\n",
        "      y.append(tok_id)\n",
        "    return x, y "
      ],
